{
   "background": {
      "scripts": [
        "lib/backend_utils.js",
        "lib/clipboard.js",
        "background_scripts/settings.js",
        // "background_scripts/sync.js",
        "background_scripts/commands.js",
        "background_scripts/exclusions.js",
        "background_scripts/completion.js",
        "background_scripts/marks.js",
        "background_scripts/main.js"
      ]
   }, /*/
   "browser_action": {
      "default_icon": "img/icons/browser_action_enabled.png",
      "default_title": "Vimium++",
      "default_popup": "pages/popup.html"
   }, //*/
   "commands": {
      "createTab": {
         "suggested_key": {
            "default": "Alt+T"
         },
         "description": "Open New Tab URL in vim++"
      },
      "reloadTab": {
         "suggested_key": {
            "default": "Alt+R"
         },
         "description": "Reload current tab"
      },
      "previousTab": {
         "suggested_key": {
            "default": "Alt+C"
         },
         "description": "Go one tab left"
      },
      "nextTab": {
         "suggested_key": {
            "default": "Alt+V"
         },
         "description": "Go one tab right"
      }
   },
   "chrome_url_overrides": {
      "newtab": "pages/newtab.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content_scripts/vimium.css" ],
      "js": [
         "lib/frontend_utils.js",
         "lib/keyboard_utils.js",
         "lib/dom_utils.js",
         "lib/handler_stack.js",
         "content_scripts/link_hints.js",
         "content_scripts/vomnibar.js",
         "content_scripts/scroller.js",
         "content_scripts/marks.js",
         "content_scripts/vimium_frontend.js"
      ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "content_scripts/file_urls.css" ],
      "matches": [ "file:///*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self';",
   "description": "optimized Vimium with the Weidu new tab",
   "icons": {
      "128": "favicon.ico"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Vimium++",
   "options_page": "pages/options.html",
   "permissions": [
      "commands",
      "tabs",
      "bookmarks",
      "history",
      "clipboardRead",
      "sessions",
      "\u003Call_urls>",
      "chrome://favicon/*",
      "management",
      "contentSettings"
    ],
   "short_name": "vim++",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.49"
}
