{
  "background": {
    "scripts": [
      "background/utils.js",
      "background/clipboard.js",
      "background/settings.js",
      "background/commands.js",
      "background/exclusions.js",
      "background/completion.js",
      "background/marks.js",
      "background/main.js"
      // , "background/others.js"
    ]
  }, /*/
  "browser_action": {
    "default_icon": "img/icons/browser_action_enabled.png",
    "default_title": "Vimium++",
    "default_popup": "pages/popup.html"
  }, //*/
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "commands": {
    "createTab": {
      "suggested_key": {
        "default": "Alt+T"
      },
      "description": "Open New Tab URL in vim++"
    },
    "reloadTab": {
      "suggested_key": {
        "default": "Alt+R"
      },
      "description": "Reload current tab"
    },
    "previousTab": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Go one tab left"
    },
    "nextTab": {
      "suggested_key": {
        "default": "Alt+V"
      },
      "description": "Go one tab right"
    }
  },
  "content_scripts": [ {
    "all_frames": true,
    "css": [ "content/vimium.css" ],
    "js": [
      "lib/utils.js",
      "lib/keyboard_utils.js",
      "lib/dom_utils.js",
      "lib/handler_stack.js",
      "content/link_hints.js",
      "content/vomnibar.js",
      "content/scroller.js",
      "content/marks.js",
      "content/vimium_frontend.js"
    ],
    "matches": [ "\u003Call_urls>" ],
    "run_at": "document_start"
  }, {
    "all_frames": true,
    "css": [ "content/file_urls.css" ],
    "matches": [ "file:///*" ],
    "run_at": "document_start"
  } ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "description": "optimized Vimium with the Weidu new tab",
  "icons": {
    "128": "favicon.ico"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Vimium++",
  "options_page": "pages/options.html",
  "permissions": [
    "bookmarks",
    "chrome://favicon/*",
    "clipboardRead",
    "commands",
    "contentSettings",
    "history",
    "management",
    "notifications",
    "sessions",
    "tabs",
    "\u003Call_urls>"
   ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.49"
}
