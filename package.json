{
  "name": "vimium-c",
  "version": "1.83.4",
  "description": "A keyboard shortcut tool for keyboard-based page navigation and browser tab operations with an advanced omnibar and global shortcuts",
  "private": true,
  "config": {
    "package-lock": false,
    "safe": false
  },
  "dependencies": {
    "pngjs": "^3.4.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "clean-css": "^4.2.1",
    "eslint": "^6.8.0",
    "gulp": ">=4",
    "gulp-changed": "*",
    "gulp-clean": "*",
    "gulp-concat": "*",
    "gulp-htmlmin": "^5.0.1",
    "gulp-newer": "*",
    "gulp-print": "*",
    "gulp-rename": "*",
    "gulp-some": "*",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": ">=3.0.2",
    "rollup": "^2.6.1",
    "seedrandom": ">=3.0.1",
    "terser": "^4.7.0",
    "web-ext": "^4.2.0"
  },
  "scripts": {
    "tsc": "node scripts/tsc.js",
    "watch": "node scripts/tsc.js --watch",
    "all": "gulp tsc",
    "prepare": "node scripts/icons-to-blob.js -q",
    "w": "node scripts/tsc.js --watch",
    "a": "gulp tsc",
    "b": "gulp bg",
    "c": "gulp content",
    "f": "gulp front",
    "l": "gulp lib",
    "p": "gulp pages",
    "lint": "node ./scripts/eslint.js",
    "eslint": "node ./scripts/eslint.js",
    "words": "bash -c \"BUILD_MinCVer=${BUILD_MinCVer:-43} BUILD_BTypes=${BUILD_BTypes:-1}; gulp words",
    "build": "gulp build",
    "dist": "npm run chrome-latest",
    "rebuild": "gulp rebuild",
    "legacy": "bash -c \"BUILD_MinCVer=${BUILD_MinCVer:-32} BUILD_BTypes=1 LOCAL_DIST=${LOCAL_DIST:-dist} BUILD_NDEBUG=0 gulp local2\"",
    "latest": "bash -c \"BUILD_MinCVer=77 BUILD_BTypes=1 LOCAL_DIST=${LOCAL_DIST:-dist} BUILD_NDEBUG=0 gulp local2\"",
    "latest-ff": "bash -c \"BUILD_BTypes=2 LOCAL_DIST=${LOCAL_DIST:-dist} BUILD_NDEBUG=0 gulp local2\"",
    "latest-fx": "npm run latest-ff",
    "latest-edge": "bash -c \"BUILD_MinCVer=53 BUILD_BTypes=4 LOCAL_DIST=${LOCAL_DIST:-dist} BUILD_NDEBUG=0 gulp local2\"",
    "chrome": "bash -c \"BUILD_MinCVer=43 BUILD_BTypes=1 NEED_COMMIT=1 gulp dist && IN_DIST=1 ./scripts/make.sh\"",
    "csize": "bash -c \"BUILD_MinCVer=${BUILD_MinCVer:-43} BUILD_BTypes=${BUILD_BTypes:-1} gulp size/content\"",
    "chrome-latest": "bash -c \"BUILD_MinCVer=77 BUILD_BTypes=1 gulp dist\"",
    "chrome-legacy": "bash -c \"BUILD_MinCVer=32 BUILD_BTypes=1 gulp dist\"",
    "chromium": "npm run chrome",
    "firefox": "bash -c \"BUILD_MinFFVer=63 BUILD_PContentSettings=0 BUILD_BTypes=2 NEED_COMMIT=1 gulp dist && IN_DIST=1 ./scripts/make.sh\"",
    "edge-c": "bash -c \"BUILD_MinCVer=77 BUILD_BTypes=1 BUILD_EdgeC=1 NEED_COMMIT=1 gulp dist && IN_DIST=1 BUILD_EdgeC=1 ./scripts/make.sh\"",
    "edge-i": "npm run edge-c",
    "edge-insider": "npm run edge-c",
    "edge-e": "npm run latest-edge",
    "clean": "gulp clean",
    "nclean": "bash -c \"gulp clean\"",
    "local": "gulp local",
    "start": "gulp local",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gdh1995/vimium-c.git"
  },
  "keywords": [
    "vimium-c",
    "vimium",
    "webextension",
    "chrome-extension",
    "shortcut",
    "keyboard",
    "chrome",
    "firefox",
    "edge"
  ],
  "author": "gdh1995",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdh1995/vimium-c/issues"
  },
  "homepage": "https://github.com/gdh1995/vimium-c"
}
