{
  "name": "vimium-c",
  "version": "1.72.0",
  "description": "customized Vimium in C-Style code, supporting Chinese, global commands and injection into some other extensions",
  "private": true,
  "config": {
    "package-lock": false
  },
  "dependencies": {
    "typescript": "next"
  },
  "devDependencies": {
    "gulp": ">=4",
    "gulp-changed": "*",
    "gulp-clean": "*",
    "gulp-concat": "*",
    "gulp-newer": "*",
    "gulp-print": "*",
    "gulp-rename": "*",
    "gulp-some": "*",
    "gulp-sourcemaps": "*",
    "gulp-typescript": "*",
    "gulp-uglify": ">=3",
    "terser": "*"
  },
  "scripts": {
    "tsc": "node scripts/tsc.js",
    "all": "node scripts/tsc.js all",
    "a": "node scripts/tsc.js all",
    "b": "node scripts/tsc.js background",
    "c": "node scripts/tsc.js content",
    "f": "node scripts/tsc.js front",
    "l": "node scripts/tsc.js lib",
    "p": "node scripts/tsc.js pages",
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "zip": "bash -c \"ENABLE_SOURCE_MAP=0 gulp dist && IN_DIST=1 WITH_MAP=0 ./scripts/make.sh\"",
    "zip-d": "bash -c \"DEBUG=1 ENABLE_SOURCE_MAP=1 gulp dist && IN_DIST=1 WITH_MAP=1 ./scripts/make.sh\"",
    "clean": "gulp clean",
    "local": "gulp local",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gdh1995/vimium-c.git"
  },
  "keywords": [
    "vimium-c",
    "vimium",
    "webextension",
    "chrome-extension",
    "chrome",
    "firefox",
    "utility"
  ],
  "author": "gdh1995",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdh1995/vimium-c/issues"
  },
  "homepage": "https://github.com/gdh1995/vimium-c#readme"
}
