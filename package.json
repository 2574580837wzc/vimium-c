{
  "name": "vimium-plus",
  "version": "1.63.0",
  "description": "optimized Vimium supporting Chinese, global commands and injection into other extensions",
  "dependencies": {
    "typescript": "next"
  },
  "devDependencies": {
    "gulp": "*",
    "gulp-changed": "*",
    "gulp-clean": "*",
    "gulp-concat": "*",
    "gulp-newer": "*",
    "gulp-print": "*",
    "gulp-rename": "*",
    "gulp-some": "*",
    "gulp-sourcemaps": "*",
    "gulp-typescript": "*",
    "gulp-uglify": ">=3",
    "uglify-js": ">=3",
    "run-sequence": "*"
  },
  "scripts": {
    "tsc": "node scripts/tsc.js",
    "all": "node scripts/tsc.js all",
    "a": "node scripts/tsc.js all",
    "b": "node scripts/tsc.js background",
    "c": "node scripts/tsc.js content",
    "f": "node scripts/tsc.js front",
    "l": "node scripts/tsc.js lib",
    "p": "node scripts/tsc.js pages",
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "zip": "bash -c 'IN_DIST=1 WITH_MAP=0 ./scripts/make.sh'",
    "clean": "gulp clean",
    "local": "gulp local",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gdh1995/vimium-plus.git"
  },
  "keywords": [
    "vimium-plus",
    "vimium",
    "webextension",
    "chrome-extension",
    "chrome",
    "firefox",
    "utility"
  ],
  "author": "gdh1995",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdh1995/vimium-plus/issues"
  },
  "homepage": "https://github.com/gdh1995/vimium-plus#readme"
}
