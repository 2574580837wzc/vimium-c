{
  "output": {
    "ascii_only": true, // if `false` by default, '\xa0' is replaced with '\xc2\xa0', which is wrong
    "keep_quoted_props": false,
    "comments": "all", // in gulp, replaced with `removeComments ? false : "all"`
    "wrap_iife": true
  },
  "compress": {
    "sequences": false,
    "properties": true,
    "dead_code": true,
    "drop_debugger": true,
    "global_defs": {
      "NDEBUG": true
    },
    // `true` will remove `/a?/.test("")` and modify polyfill's `String(this)`
    // so "unsafe" is not what Vimium C needs
    "unsafe": false,
    "unsafe_comps": false,
    "unsafe_math": true,
    "unsafe_proto": false,
    "unsafe_regexp": false,
    "conditionals": true,
    "comparisons": true,
    "evaluate": true,
    "booleans": false,
    "loops": true,
    "unused": false,
    "toplevel": false,
    "hoist_funs": true,
    "hoist_vars": true,
    "if_return": true,
    "inline": false,
    "join_vars": true,
    "collapse_vars": true,
    "warnings": true,
    "negate_iife": false,
    "pure_getters": true,
    "keep_fargs": true, // typescript will check unused arguments
    "keep_fnames": false,
    "keep_infinity": false,
    "side_effects": true
  },
  "mangle": {
    "properties": {
      "regex": "/^_|_$/"
    },
    "reserved": [
      // # background global names:
      "Utils", "NotChrome", "IsEdge", "IsFirefox", "Settings", "Backend",
      "Commands", "CommandsData", "Exclusions", "HelpDialog",
      // # content global names:
      "VimiumInjector", "VUtils", "VKeyboard", "VDom",
      "VHints", "VOmni", "VScroller", "VMarks", "VFind", "VVisual",
      "VSettings", "VHUD", "VPort", "VEvent",
      // # front/vomnibar
      "VCID",
      // # inner names:
      "startsWith", "endsWith", "addEventListener", "toString",
      "Suggestion", "VC"
    ], // for polyfills and backend Suggestion
    "toplevel": true
  }
}