{
  "extends": "tslint:recommended",
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "align": [
      true,
      "statements"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "class-name": false,
    "forin": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^(//|/\\*| \\*)"
      }
    ],
    "member-access": false,
    "member-ordering": false,
    "no-angle-bracket-type-assertion": false,
    "no-bitwise": false,
    "no-conditional-assignment": false,
    "no-console": false,
    "no-empty": [
      true,
      "allow-empty-catch"
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "no-reference": false,
    "no-shadowed-variable": [
      true,
      {
        "class": true,
        "enum": true,
        "function": false,
        "interface": true,
        "namespace": true,
        "typeAlias": true,
        "typeParameter": true
      }
    ],
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "no-var-keyword": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-catch",
      "check-finally"
    ],
    "one-variable-per-declaration": false,
    "only-arrow-functions": false,
    "prefer-const": false,
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "trailing-comma": [
      true,
      "never"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case",
      "allow-snake-case",
      "allow-trailing-underscore"
    ]
  },
  "rulesDirectory": []
}